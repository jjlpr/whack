.container {
  width: 100%;
  outline: none;
  color: white;
  text-decoration: none;
  text-align: center;
  background: transparent;
  border: none;
}

.url {
  display: block;
  margin-bottom: 15px;
  font-size: 21px;
  text-decoration: underline;
  word-wrap: break-word;
}

.instructions {
  display: block;
  width: 100%;
  margin-bottom: 15px;
  font-size: 14px;
  color: $lightBrown;
  text-transform: uppercase;
  text-align: center;
}
