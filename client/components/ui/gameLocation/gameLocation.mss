.container {
  width: 100%;
  padding-top: 14px;
  margin-bottom: 20px;
  outline: none;
  color: white;
  text-decoration: none;
  text-align: center;
  background: transparent;
  border: none;
}

.title {
  display: block;
  margin-bottom: 14px;
  font-size: 18px;
}

.url {
  display: block;
  margin-bottom: 46px;
  font-size: 25px;
  text-decoration: underline;
  word-wrap: break-word;
}

.instructions {
  display: block;
  width: 100%;
  margin-bottom: 19px;
  font-size: 18px;

}
