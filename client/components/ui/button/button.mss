.button {
  position: relative;
  bottom: 0;
  line-height: 66px;
  width: 100%;
  background-color: #B38ED6;
  border: 6px solid;
  border-radius: 66px;
  color: white;
  font-size: 25px;
  box-shadow: 4px 5px 25px 0px rgba(181,163,199,1);
  outline: none;
}

.button:active {
  background-color: #5F4677;
}
