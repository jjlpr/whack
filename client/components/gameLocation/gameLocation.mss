.container {
  display: inline-block;
  box-sizing: border-box;
  width: 100%;
  padding-top: 14px;
  background-color: #5F4677;
  border: 6px solid white;
  outline: none;
  color: white;
  text-decoration: none;
  text-align: center;
  box-shadow: 4px 5px 0px 0px rgba(181,163,199,0.2);
}
.container:active {
  background-color: #423153;
  box-shadow: 4px 5px 0px 0px rgba(181,163,199,0.5);
}

.title {
  display: inline-block;
  width: 100%;
  margin-bottom: 14px;
  font-size: 18px;
}

.url {
  display: inline-block;
  width: 100%;
  margin-bottom: 46px;
  font-size: 25px;
  text-decoration: underline;
}

.instructions {
  display: inline-block;
  width: 100%;
  margin-bottom: 19px;
  font-size: 18px;

}